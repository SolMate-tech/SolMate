# Server Configuration
PORT=5000
NODE_ENV=development
LOG_LEVEL=info

# Solana Configuration
SOLANA_NETWORK=mainnet-beta
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com

# Database Configuration (for future implementation)
DB_HOST=localhost
DB_PORT=27017
DB_NAME=solmate
DB_USER=solmate_user
DB_PASSWORD=your_password

# JWT Authentication (for future implementation)
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRES_IN=7d

# AI Service Configuration (for future implementation)
AI_API_KEY=your_ai_api_key
AI_MODEL=gpt-4

# External API Keys (for future implementation)
JUPITER_API_KEY=your_jupiter_api_key
SOLSCAN_API_KEY=your_solscan_api_key

# MongoDB configuration
MONGODB_URI=mongodb://localhost:27017/solmate
